  tprDeHighPStopTargTriggerInfoMerger: {
    module_type: "MergeTriggerInfo"
  }
  tprDeHighPTriggerInfoMerger: {
    module_type: "MergeTriggerInfo"
  }
  tprDeLowPStopTargTriggerInfoMerger: {
